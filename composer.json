{
    "name": "pchec/cakephp-inheritance",
    "description": "CakePHP plugin with Single Table Inheritance behavior",
    "keywords":[
        "cakephp",
        "inheritance"
    ],
   "homepage": "http://github.com/pchec/cakephp-inheritance",
    "authors":[
        {
            "name":"Piotr Chec",
            "role":"Author"
        }
    ],
    "type": "cakephp-plugin",
    "license": "MIT",
    "support":{
        "source":"https://github.com/pchec/cakephp-inheritance",
        "issues":"https://github.com/pchec/cakephp-inheritance/issues",
    },
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Inheritance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inheritance\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
